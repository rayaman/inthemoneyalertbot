    <script>
        var sold = "{{sold}}"
        var hammer = "{{hammer}}"
        var modalBtns = [...document.querySelectorAll(".button")];
        modalBtns.forEach(function(btn){
            btn.onclick = function() {
            var modal = btn.getAttribute('data-modal');
            var elem = document.getElementById(modal)
            elem.style.display = "block";
            elem.scrollIntoView();
            }
        });
        var closeBtns = [...document.querySelectorAll(".closeme")];
        closeBtns.forEach(function(btn){
            btn.onclick = function() {
            var modal = btn.closest('.modal');
            modal.style.display = "none";
            }
        });
        function logSubmit(event) {
            console.log(`Form Submitted! Time stamp: ${event.timeStamp}`)
            var xhr = new XMLHttpRequest();
            xhr.open("POST", "/lotform", true);
            xhr.setRequestHeader('Content-Type', 'application/json');
            xhr.send(JSON.stringify({
                subject: "Condition Report",
                time: event.timeStamp,
                url: window.location.href,
                name: name.value,
                email: email.value,
                phone: phone.value,
                message: message.value,
                lot: "{{lot}}"
            }));
            alert.style.display = "block"
            open.style.display = "none"
            mainform.style.display = "none";
            event.preventDefault();
        }
        function logSubmit2(event) {
            console.log(`Form Submitted! Time stamp: ${event.timeStamp}`)
            var xhr = new XMLHttpRequest();
            xhr.open("POST", "/bidform", true);
            xhr.setRequestHeader('Content-Type', 'application/json');
            xhr.send(JSON.stringify({
                subject: "I'd Like to place a bid",
                time: event.timeStamp,
                url: window.location.href,
                bid: bid.value,
                name: name2.value,
                email: email2.value,
                phone: phone2.value,
                lot: "{{lot}}"
            }));
            window.alert("Your bid has been sent to the Auction House, a staff member will soon get in contact with you!")
            open2.style.display = "none"
            mainform2.style.display = "none";
            event.preventDefault();
        }
        const mainform = document.getElementById('modalOne')
        const mainform2 = document.getElementById('modalTwo')
        const form = document.getElementById('form')
        const form2 = document.getElementById('form2')
        const open = document.getElementById('open')
        const alert = document.getElementById('alert')
        const open2 = document.getElementById('open2')
        const alertbid = document.getElementById('alertbid')
        const name = document.getElementById('form_name')
        const email = document.getElementById('form_email')
        const phone = document.getElementById('form_phone')

        const isold = document.getElementById('isold')
        const est = document.getElementById('est')

        const name2 = document.getElementById('form_name2')
        const email2 = document.getElementById('form_email2')
        const phone2 = document.getElementById('form_phone2')

        const message = document.getElementById('form_message')
        const bid = document.getElementById('form_bid')
        if(sold!=""){
            open2.style.display = "none"
            isold.style.display = "block"
            est.style.display = "none"
        }
        form.addEventListener('submit', logSubmit);
        form2.addEventListener('submit', logSubmit2);
        window.onclick = function(event) {
            if (event.target.className === "modal") {
            event.target.style.display = "none";
            }
        }
    </script>

