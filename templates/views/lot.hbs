    <form class="search" style="padding-top: 70px;margin-left: auto;max-width:300px;">
        <input type="text" placeholder="#" name="" style="width:50px" id="gotoIn" onkeypress="return test(event)">
        <button type="button" onclick="gotolot(event)" class="btn btn-primary display-4">Go to Lot</button>
    </form>
    <script>
        const $Lot = document.querySelector("form input[id='gotoIn']")
        function gotolot(event) {
            var num = Number($Lot.value)
            if (num) {
                window.location.href = `/lot?lot=${$Lot.value}&auction={{id}}`
            }
        }
        function test(event) {
            if (event.which == 13 || event.keyCode == 13) {
                gotolot(event)
                return false;
            }
            return true;
        };
    </script>
